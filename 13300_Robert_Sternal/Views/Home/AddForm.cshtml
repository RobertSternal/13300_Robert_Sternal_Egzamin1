@{
}

<form asp-action="AddTravel" asp-controller="Home">
    Nazwa podróży
    <input type="text" asp-for="TravelName" class="form-control" />
    Długość podróży (km)
    <input type="text" asp-for="TravelLength" class="form-control" />
    Zużyte paliwo (l)
    <input type="text" asp-for="FuelUsed" class="form-control" />
    Cena paliwa (zł)
    <input type="text" asp-for="FuelPrice" class="form-control" />


    <button type="submit">Dodaj</button>
</form>